{
  "extends": ["tslint:latest"],
  "rules": {
    "no-floating-promises": true,
    "no-var-requires": false,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      false
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "arrow-parens": false,
    "no-console": false
  }
}
