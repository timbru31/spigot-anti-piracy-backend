{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "arrow-parens": false,
    "no-console": false,
    "no-floating-promises": true,
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-var-requires": false,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      false
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  }
}
