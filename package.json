{
  "name": "spigot-anti-piracy-backend",
  "version": "0.6.0",
  "description": "A koajs application to blacklist leaked plugins",
  "bin": "./dist/app.js",
  "main": "./src/app.js",
  "scripts": {
    "babel-node": "./node_modules/babel-cli/bin/babel-node.js",
    "build": "babel ./src/app.js -d dist",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 -p eslint",
    "coverage": "npm run babel-node -- ./node_modules/babel-istanbul/lib/cli.js cover _mocha",
    "lint": "eslint --ignore-pattern dist --ignore-pattern coverage ./src",
    "prepublish": "npm run build",
    "start": "node dist/app.js",
    "release": "standard-version",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-core/register",
    "watch": "nodemon --exec npm run babel-node -- ./src/app.js"
  },
  "engines": {
    "node": ">=4.2"
  },
  "author": {
    "name": "timbru31"
  },
  "dependencies": {
    "babel-cli": "~6.23.0",
    "babel-core": "~6.23.1",
    "babel-plugin-transform-async-to-generator": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-stage-3": "~6.22.0",
    "babel-runtime": "~6.23.0",
    "koa": "~2.0.0",
    "koa-bodyparser": "~3.2.0",
    "koa-router": "~7.1.0",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "~7.1.1",
    "babel-istanbul": "~0.12.1",
    "codeclimate-test-reporter": "~0.4.1",
    "conventional-changelog-cli": "~1.2.0",
    "coveralls": "~2.11.16",
    "cz-customizable": "~4.0.0",
    "eslint": "~3.15.0",
    "mocha": "~3.2.0",
    "nodemon": "~1.11.0",
    "should": "~11.2.0",
    "standard-version": "~4.0.0",
    "supertest": "~3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "license": "MIT",
  "repository": "timbru31/spigot-anti-piracy-backend",
  "bugs": {
    "url": "https://github.com/timbru31/spigot-anti-piracy-backend/issues"
  },
  "keywords": [
    "spigot",
    "bukkit",
    "minecraft",
    "ban",
    "leak"
  ]
}
