{
  "name": "spigot-anti-piracy-backend",
  "version": "2.0.0",
  "description": "A koajs application to blacklist leaked plugins",
  "bin": "dist/app.js",
  "main": "dist/app.js",
  "scripts": {
    "coverage": "nyc report --reporter=lcov",
    "lint": "tslint -p tsconfig.test.json -e '*.txt' '{src,test}/**/*.ts'",
    "start": "ts-node src/app.ts",
    "prerelease": "npm run test && rm -rf dist && tsc",
    "release": "standard-version -s",
    "pretest": "npm run lint",
    "test": "nyc mocha",
    "watch": "nodemon --exec 'ts-node' src/app.ts"
  },
  "engines": {
    "node": ">=12"
  },
  "author": "Tim Brust <github@timbrust.de>",
  "dependencies": {
    "@koa/router": "^8.0.8",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/koa": "^2.11.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.0",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.12.26",
    "@types/supertest": "^2.0.8",
    "coveralls": "^3.0.9",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "should": "^13.2.3",
    "standard-version": "^7.1.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "license": "MIT",
  "repository": "timbru31/spigot-anti-piracy-backend",
  "bugs": {
    "url": "https://github.com/timbru31/spigot-anti-piracy-backend/issues"
  },
  "keywords": [
    "spigot",
    "bukkit",
    "minecraft",
    "ban",
    "leak"
  ],
  "standard-version": {
    "scripts": {
      "postchangelog": "npx prettier --write CHANGELOG.md"
    }
  }
}
