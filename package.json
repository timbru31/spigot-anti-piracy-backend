{
  "name": "spigot-anti-piracy-backend",
  "version": "1.0.2",
  "description": "A koajs application to blacklist leaked plugins",
  "bin": "./src/app.js",
  "main": "./src/app.js",
  "scripts": {
    "coverage": "nyc report --reporter=lcov",
    "lint": "eslint --ignore-pattern coverage ./src",
    "start": "node ./src/app.js",
    "release": "standard-version",
    "pretest": "npm run lint",
    "test": "nyc mocha",
    "watch": "nodemon ./src/app.js"
  },
  "engines": {
    "node": ">=8"
  },
  "author": {
    "name": "timbru31"
  },
  "dependencies": {
    "koa": "~2.3.0",
    "koa-bodyparser": "~4.2.0",
    "koa-router": "~7.2.1",
    "winston": "~3.0.0-rc0"
  },
  "devDependencies": {
    "codacy-coverage": "~2.0.2",
    "codeclimate-test-reporter": "~0.5.0",
    "coveralls": "~3.0.0",
    "cz-customizable": "~5.2.0",
    "eslint": "~4.8.0",
    "mocha": "~3.5.0",
    "nodemon": "~1.12.0",
    "nyc": "^11.0.2",
    "should": "~13.1.0",
    "standard-version": "~4.2.0",
    "supertest": "~3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "license": "MIT",
  "repository": "timbru31/spigot-anti-piracy-backend",
  "bugs": {
    "url": "https://github.com/timbru31/spigot-anti-piracy-backend/issues"
  },
  "keywords": [
    "spigot",
    "bukkit",
    "minecraft",
    "ban",
    "leak"
  ]
}
