{
  "name": "spigot-anti-piracy-backend",
  "version": "1.0.0",
  "description": "A koajs application to blacklist leaked plugins",
  "bin": "./src/app.js",
  "main": "./src/app.js",
  "scripts": {
    "coverage": "istanbul cover _mocha",
    "lint": "eslint --ignore-pattern coverage ./src",
    "start": "node ./src/app.js",
    "release": "standard-version",
    "pretest": "npm run lint",
    "test": "mocha",
    "watch": "nodemon ./src/app.js"
  },
  "engines": {
    "node": ">=7.6"
  },
  "author": {
    "name": "timbru31"
  },
  "dependencies": {
    "koa": "~2.2.0",
    "koa-bodyparser": "~4.1.0",
    "koa-router": "~7.1.0",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "codacy-coverage": "~2.0.1",
    "codeclimate-test-reporter": "~0.4.1",
    "coveralls": "~2.12.0",
    "cz-customizable": "~4.0.0",
    "eslint": "~3.18.0",
    "istanbul": "~1.1.0-alpha.1",
    "mocha": "~3.2.0",
    "nodemon": "~1.11.0",
    "should": "~11.2.0",
    "standard-version": "~4.0.0",
    "supertest": "~3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "license": "MIT",
  "repository": "timbru31/spigot-anti-piracy-backend",
  "bugs": {
    "url": "https://github.com/timbru31/spigot-anti-piracy-backend/issues"
  },
  "keywords": [
    "spigot",
    "bukkit",
    "minecraft",
    "ban",
    "leak"
  ]
}
